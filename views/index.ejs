   <style>
      body {
         overflow-x: hidden;
         font-family: "Special Elite", monospace;
      }

      #top-bar {
         background-color: #212F3C;
         width: 100%;
         height: 60px;
         background-color: #212F3C;
         position: relative;
         top: -20px;
      }

      #title-top-bar {
         position: relative;
         top: 5px;
         color: white;
         font-weight: bold;
         left: 55px;
      }

      #logo-top-bar {
         width: 60px;
         position: relative;
         top: -50px;
      }

      .line {
         border-right: 2px solid rgba(255, 255, 255, 0.75);
         color: rgba(255, 255, 255, 0.75);
         font-size: 3em;
         margin: 0 auto;
         white-space: nowrap;
         overflow: hidden;
      }

      .typing-animation {
         animation: blinkCursor 500ms steps(40) infinite normal,
            typing 4s steps(40) 1s normal both;
      }

      @keyframes typing {
         from {
            width: 0;
         }

         to {
            width: 6em;
         }
      }

      @keyframes blinkCursor {
         from {
            border-right-color: rgba(255, 255, 255, 0.75);
         }

         to {
            border-right-color: transparent;
         }
      }
   </style>
